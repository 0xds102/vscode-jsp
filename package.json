{
    "name": "vscode-jsp-clone",
    "displayName": "Java Server Pages (JSP) Language Support",
    "description": "JSP language support for Visual Studio Code, ported from TextMate's JSP bundle.",
    "version": "0.0.5",
    "publisher": "J0hnMilt0n",
    "repository": {
        "type": "git",
        "url": "https://github.com/J0hnMilt0n/vscode-jsp"
    },
    "icon": "java.jpeg",
    "author": {
        "name": "J0hnMilt0n",
        "url": "https://github.com/J0hnMilt0n/",
        "email": "mjohnmilton@gmail.com"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jsp",
                "aliases": [
                    "Java Server Pages",
                    "jsp",
                    "JSP",
                    "jstl",
                    "JSTL"
                ],
                "extensions": [
                    ".tag",
                    ".jsp",
                    ".jspf"
                ],
                "icon": {
                "light": "jsp.png",
                "dark": "jsp.png"
            },
                "configuration": "./jsp-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jsp",
                "scopeName": "text.html.jsp",
                "path": "./syntaxes/jsp.tmLanguage.json",
                "embeddedLanguages": {
                    "source.css": "css",
                    "source.js": "javascript"
                }
            }
        ]
    }
}
